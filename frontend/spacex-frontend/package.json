    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "postcss": "^8.4.28",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "tailwindcss": "^3.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
